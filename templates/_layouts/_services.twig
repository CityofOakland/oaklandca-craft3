{% extends '_layout.twig' %}

{% block content %}
  {% include '_partials/_page-banners.twig' with {
    breadCrumbLinks: [
      {
        title: 'Services',
        link: '/services'
      }
    ],
  } %}
  <section class="bg-white relative z-0">
    <div class="container pt-0">
      {% include '_partials/_elements/_ctas.twig' %}
      <div class="flex">
      {% include "_partials/_elements/_nav-sidebar.twig" %}
      <div class="flex-1">
        {% include "_partials/_elements/_page-alerts.twig" %}
        {% include '_partials/_content-builder/_tool.twig' %}
        {% if entry.embeddedContent.exists() %}
          {{ entry.embeddedContent.one.codeInsertion | raw }}
        {% endif %}
      </div>
      </div>
    </div>
  </section>
{% endblock %}
